        "bootstrap": "^4.6.0",
        "cross-env": "^7.0",
        "expose-loader": "^2.0.0",
        "laravel-mix": "^6.0.19",
        "lodash": "^4.17.19",
        "node-sass": "^5.0.0",
        "resolve-url-loader": "^2.3.2",
        "sass": "^1.32.8",
        "webpack": "^5.31.2"
    },
    "dependencies": {
        "@popperjs/core": "^2.5.4",
        "jquery": "^3.5.1",

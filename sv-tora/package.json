{
    "name": "sv-tora",
    "version": "0.1.0",
    "description": "CMS - Competition Management System for SV Tora",
    "author": "Markus St√§dler",
    "private": true,
    "babel": {
        "presets": [
            "@babel/preset-env"
        ]
    },
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",

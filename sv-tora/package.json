        "babel-loader": "^8.2.3",
        "cross-env": "^7.0",
        "expose-loader": "^3.1.0",
        "laravel-mix": "^6.0.39",
        "lodash": "^4.17.21",
        "node-sass": "^6.0.1",
        "resolve-url-loader": "^4.0.0",
        "sass": "^1.43.5",
        "webpack": "^5.64.4"
    },
    "dependencies": {
        "@popperjs/core": "^2.10.2",
        "jquery": "^3.6.0",
        "regenerator-runtime": "^0.13.9",

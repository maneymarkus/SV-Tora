{
    "name": "sv-tora",
    "version": "0.1.0",
    "description": "CMS - Competition Management System for SV Tora",
    "author": "Markus St√§dler",
    "private": true,
    "babel": {
        "presets": [
            "@babel/preset-env"
        ]
    },
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "webpack": "npm run webpack_dev",
        "webpack_dev": "webpack --config webpack.dev.config.js",
        "webpack_prod": "webpack --config webpack.prod.config.js"
    },
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@babel/plugin-proposal-class-properties": "^7.16.0",
        "@babel/preset-env": "^7.16.4",

        "prod": "npm run production",
        "production": "mix --production",
        "webpack": "npm run webpack_dev",
        "webpack_dev": "webpack --config webpack.dev.config.js",
        "webpack_prod": "webpack --config webpack.prod.config.js"
    },
    "devDependencies": {
        "@babel/core": "^7.13.15",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/preset-env": "^7.13.15",
